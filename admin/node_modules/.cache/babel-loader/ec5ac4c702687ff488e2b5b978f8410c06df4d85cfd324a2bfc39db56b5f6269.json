{"ast":null,"code":"export default {\n  name: \"AddAnswers\",\n  props: ['questionId'],\n  methods: {\n    addAnswer() {\n      const answer = {};\n      answer['id'] = this.answers.length + 1;\n      answer['title'] = null;\n      answer['value'] = null;\n      this.$store.commit('addAnswer', {\n        answer,\n        questionId: this.questionId\n      });\n    },\n    removeAnswer(id) {\n      this.$store.commit('removeAnswer', {\n        id,\n        questionId: this.questionId\n      });\n    },\n    giveData(answer, id) {\n      this.$store.commit('editAnswer', {\n        answer,\n        id,\n        questionId: this.questionId\n      });\n    }\n  },\n  computed: {\n    answers() {\n      return this.$store.getters.question(this.questionId).answers;\n    }\n  }\n};","map":{"version":3,"mappings":"AA+BA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE,CACL,YAAW,CACZ;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,MAAMC,MAAK,GAAI,CAAC;MAChBA,MAAM,CAAC,IAAI,IAAI,IAAI,CAACC,OAAO,CAACC,MAAK,GAAI;MACrCF,MAAM,CAAC,OAAO,IAAI,IAAG;MACrBA,MAAM,CAAC,OAAO,IAAI,IAAG;MACrB,IAAI,CAACG,MAAM,CAACC,MAAM,CAAC,WAAW,EAAE;QAC9BJ,MAAM;QACNK,UAAU,EAAE,IAAI,CAACA;MACnB,CAAC;IACH,CAAC;IACDC,YAAY,CAACC,EAAE,EAAE;MACf,IAAI,CAACJ,MAAM,CAACC,MAAM,CAAC,cAAc,EAAE;QACjCG,EAAE;QACFF,UAAU,EAAE,IAAI,CAACA;MACnB,CAAC;IACH,CAAC;IACDG,QAAQ,CAACR,MAAM,EAAEO,EAAE,EAAE;MACnB,IAAI,CAACJ,MAAM,CAACC,MAAM,CAAC,YAAY,EAAE;QAC/BJ,MAAM;QACNO,EAAE;QACFF,UAAU,EAAE,IAAI,CAACA;MACnB,CAAC;IACH;EACF,CAAC;EACDI,QAAQ,EAAE;IACRR,OAAO,GAAG;MACR,OAAO,IAAI,CAACE,MAAM,CAACO,OAAO,CAACC,QAAQ,CAAC,IAAI,CAACN,UAAU,CAAC,CAACJ,OAAM;IAC7D;EACF;AACF","names":["name","props","methods","addAnswer","answer","answers","length","$store","commit","questionId","removeAnswer","id","giveData","computed","getters","question"],"sourceRoot":"","sources":["/home/yan/Документы/psyreply2.0_front/admin/src/components/Test/AddAnswers.vue"],"sourcesContent":["<template>\n  <article class=\"wrapper\">\n    <y-modal class=\"modal\">\n      <header class=\"header\">\n        <y-left-arrow-button @click=\"$emit('close')\" />\n        <h1 class=\"heading\">Добавить ответы</h1>\n      </header>\n\n      <template v-if=\"answers.length > 0\">\n        <article v-for=\"(answer, id) of answers\" class=\"answers__add\" >\n          <y-input @input=\"giveData(answer, id)\" v-model=\"answer.title\" placeholder=\"Введите ответ\"></y-input>\n          <div class=\"question__coins\">\n            <label class=\"label\">Баллы за ответ</label>\n            <y-input @input=\"giveData(answer, id)\" v-model=\"answer.value\" class=\"question__input\" />\n          </div>\n          <y-button @click=\"removeAnswer(id)\" class=\"question__del\">X</y-button>\n        </article>\n      </template>\n\n      <hr/>\n\n      <div class=\"anw__add__button\">\n        <button @click=\"addAnswer\" class=\"plus\">+</button>\n      </div>\n\n      <y-cool-button @click=\"$emit('close')\">Сохранить и вернтуься</y-cool-button>\n    </y-modal>\n  </article>\n</template>\n\n<script>\nexport default {\n  name: \"AddAnswers\",\n  props: [\n    'questionId'\n  ],\n  methods: {\n    addAnswer() {\n      const answer = {}\n      answer['id'] = this.answers.length + 1\n      answer['title'] = null\n      answer['value'] = null\n      this.$store.commit('addAnswer', {\n        answer,\n        questionId: this.questionId\n      })\n    },\n    removeAnswer(id) {\n      this.$store.commit('removeAnswer', {\n        id,\n        questionId: this.questionId\n      })\n    },\n    giveData(answer, id) {\n      this.$store.commit('editAnswer', {\n        answer,\n        id,\n        questionId: this.questionId\n      })\n    }\n  },\n  computed: {\n    answers() {\n      return this.$store.getters.question(this.questionId).answers\n    }\n  }\n}\n</script>\n\n<style scoped>\n.wrapper {\n  position: fixed;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  backdrop-filter: blur(5px);\n  background-color: rgba(0, 0, 0, 0.42);\n}\n.header {\n  display: grid;\n  grid-template-columns: auto 1fr min-content;\n  grid-gap: 2rem;\n  justify-content: start;\n  align-items: center;\n}\n.heading {\n  font-size: 1.5rem;\n}\n.answers__add{\n  display: grid;\n  grid-template-columns: 60% 1fr 1fr;\n  align-items: center;\n  margin-top: 50px;\n  grid-gap: 1.875rem;\n}\n.label {\n  font-size: 1.5rem !important;\n  margin-right: 12px;\n}\n.question__coins {\n  display: grid;\n  grid-template-columns: max-content minmax(50px, 100px);\n  align-items: center;\n  grid-gap: .3em;\n}\n.question__input {\n  padding: .3em;\n}\n.question__del {\n  color: red;\n  width: min-content;\n}\nhr {\n  margin-top: 30px;\n  margin-bottom: 30px;\n  min-width: 30vw;\n  border-left: 0px solid white;\n  border-right: 0px solid white;\n  border-bottom: 0px solid white;\n  border-top: 1px solid rgba(255, 255, 255, 0.52);\n}\n.anw__add__button{\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.plus{\n  margin-left:10px ;\n  background: rgba(255, 255, 255, 0.17);\n  border-radius: 7px;\n  font-family: 'Rubik';\n  font-style: normal;\n  font-weight: 500;\n  height: auto;\n  align-items: center;\n  text-align: center;\n  color: #FFFFFF;\n  cursor: pointer;\n  width: 36px;\n  line-height: 34px;\n  font-size: 32px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}