ustify-content: center;\n  background: linear-gradient(140.62deg, rgba(255, 255, 255, 0.25) 2.81%, rgba(255, 255, 255, 0.1) 100.82%);\n  box-shadow: 0px 4px 52px rgba(143, 0, 255, 0.11);\n  border-radius: 16px;\n  padding-top: 1rem;\n  padding-bottom: 4rem;\n  width: 30rem;\n  border-top: rgba(255, 255, 255, 0.5);\n  border-bottom:rgba(255, 255, 255, 0.5) ;\n  border-right:rgba(255, 255, 255, 0);\n  border-left:rgba(255, 255, 255, 0) ;\n\n}\n.popup__title {\n\n  margin-left: 1rem;\n\n}\n.popup__icon {\n  margin-top: 2rem;\n  width: 10rem;\n\n}\n.popup__fix {\n  width: 20rem;\n}\n.popup__exit {\n  align-self: end;\n}\n.popup__exit:hover {\n  cursor: pointer;\n}\n.error__code__title{\n  margin-top: 0.5rem;\n  opacity: 0.7;\n  font-size: 0.9rem;\n}\n.error__code{\n  font-size: 0.9rem;\n  margin-top: 0.5rem;\n  color:rgba(251, 128, 255, 0.58);\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}