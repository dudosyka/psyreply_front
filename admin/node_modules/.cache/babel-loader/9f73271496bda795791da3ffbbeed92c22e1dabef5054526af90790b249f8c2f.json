{"ast":null,"code":"import User from \"@/api/admin/User\";\nexport default {\n  name: \"YResultsListItem\",\n  props: {\n    name: {\n      default: 'no name'\n    },\n    username: {\n      default: 'no username'\n    },\n    id: {\n      default: -1\n    },\n    date: {\n      default: '32/13/893'\n    },\n    week: {\n      default: \"3\"\n    },\n    valid: {\n      default: true\n    },\n    row: {\n      default: {}\n    },\n    selectable: {\n      default: false\n    },\n    active: {\n      default: false\n    }\n  },\n  methods: {\n    async openClientDash() {\n      const client = new User();\n      const link = await client.getDashLink(this.row.user.jetBotId).then(res => res.text());\n      window.open(link, \"_blank\");\n    }\n  },\n  computed: {\n    formattedDate() {\n      let date = new Date(this.date);\n      const day = date.getDate();\n      const month = date.getMonth() + 1;\n      const year = date.getFullYear();\n      return `${day}/${month}/${year}`;\n    }\n  }\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,IAAG,MAAO,kBAAkB;AAEnC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE;IACLD,IAAI,EAAE;MACJE,OAAO,EAAE;IACX,CAAC;IACDC,QAAQ,EAAE;MACRD,OAAO,EAAE;IACX,CAAC;IACDE,EAAE,EAAE;MACFF,OAAO,EAAE,CAAC;IACZ,CAAC;IACDG,IAAI,EAAE;MACJH,OAAO,EAAE;IACX,CAAC;IACDI,IAAI,EAAE;MACJJ,OAAO,EAAE;IACX,CAAC;IACDK,KAAK,EAAE;MACLL,OAAO,EAAE;IACX,CAAC;IACDM,GAAG,EAAE;MACHN,OAAO,EAAE,CAAC;IACZ,CAAC;IACDO,UAAU,EAAE;MACVP,OAAO,EAAE;IACX,CAAC;IACDQ,MAAM,EAAE;MACNR,OAAO,EAAE;IACX;EACF,CAAC;EACDS,OAAO,EAAE;IACP,MAAMC,cAAc,GAAG;MACrB,MAAMC,MAAK,GAAI,IAAId,IAAI,EAAE;MACzB,MAAMe,IAAG,GAAI,MAAMD,MAAM,CAACE,WAAW,CAAC,IAAI,CAACP,GAAG,CAACQ,IAAI,CAACC,QAAQ,CAAC,CAACC,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACC,IAAI,EAAE,CAAC;MACrFC,MAAM,CAACC,IAAI,CAACR,IAAI,EAAE,QAAQ;IAC5B;EACF,CAAC;EACDS,QAAQ,EAAE;IACRC,aAAa,GAAG;MACd,IAAInB,IAAG,GAAI,IAAIoB,IAAI,CAAC,IAAI,CAACpB,IAAI;MAC7B,MAAMqB,GAAE,GAAIrB,IAAI,CAACsB,OAAO,EAAC;MACzB,MAAMC,KAAI,GAAIvB,IAAI,CAACwB,QAAQ,EAAC,GAAI;MAChC,MAAMC,IAAG,GAAI